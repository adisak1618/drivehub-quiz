{
  "name": "hasura",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "docker-test-up": "docker-compose -f docker-compose-testing.yaml up -d",
    "wait": "echo 'hasura:wait' && wait-on http-get://localhost:5001/healthz",
    "apply-migration-test": "hasura migrate apply  --endpoint http://localhost:5001 --database-name default --skip-update-check",
    "apply-metadata-test": "hasura metadata apply  --endpoint http://localhost:5001 --skip-update-check",
    "setup-test-db": "yarn docker-test-up && yarn wait && yarn apply-migration-test && yarn apply-metadata-test",
    "test": "jest --coverage --runInBand"
  },
  "keywords": [],
